{
  "root": true,
  "extends": ["airbnb-base", "next", "prettier"],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "plugins": ["prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2023,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "import/extensions": 0,
    "no-console": "off"
  }
}
